<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cutepikachu.biz.mapper.FileInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.cutepikachu.biz.model.entity.FileInfo">
        <id column="file_id" property="fileId" jdbcType="BIGINT"/>
        <result column="path" property="path"
                jdbcType="VARCHAR"/>
        <result column="name" property="name"
                jdbcType="VARCHAR"/>
        <result column="size" property="size"
                jdbcType="INTEGER"/>
        <result column="extension" property="extension"
                jdbcType="VARCHAR"/>
        <result column="biz_tag" property="bizTag"
                jdbcType="VARCHAR"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        `file_id`, `path`, `name`, `size`, `extension`, `biz_tag`
    </sql>

</mapper>
